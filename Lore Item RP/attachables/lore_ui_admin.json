{
	"format_version": "1.10.0",
	"minecraft:attachable": {
		"description": {
			"identifier": "da:lore_ui_admin",
			"materials": {
				"default": "entity_alphatest",
				"enchanted": "entity_alphatest_glint"
			},
			"textures": {
				"default": "textures/lore_ui/lore_item/lore_ui_admin",
				"enchanted": "textures/misc/enchanted_item_glint"
			},
			"geometry": {
				"default": "geometry.lore_ui_admin"
			},
			"animations": {
				"first_person": "animation.player.lore_ui_vanilla.first_person",
				"default": "animation.player.lore_ui_vanilla"
			},
			"scripts": {
				"animate": [
					{
						"first_person": "c.is_first_person && c.item_slot == 'main_hand' && q.get_equipped_item_name == 'lore_ui_admin'"
					},
					{
						"default": "!c.is_first_person && c.item_slot == 'main_hand' && q.get_equipped_item_name == 'lore_ui_admin'"
					}
				]
			},
			"render_controllers": [
				"controller.render.lore_ui_vanilla"
			]
		}
	}
}
